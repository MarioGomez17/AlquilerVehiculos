@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Geologica:wght@100..900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/CalificarAlquiler.css">
    <title>CALIFICAR ALQUILER</title>
</head>
<body class="BodyCalificarAlquiler">
    <div class="ContenidoBodyCalificarAlquiler">
        <div class="DivBarraNavegacionCalificarAlquiler">
            <nav class="BarraNavegacionCalificarAlquiler">
                <div class="DivLinkCalificarAlquiler">
                    <a asp-controller="Inicio" asp-action="Inicio"><img class="ImagenLogo" src="~/imagenes/Logo.webp"
                            alt="Logotipo" /></a>
                </div>
                <div class="DivLinkCalificarAlquiler">
                    <a asp-controller="Usuario" asp-action="IniciarSesion" class="LinkCalificarAlquiler">
                        <svg xmlns="http://www.w3.org/2000/svg"
                            class="icon icon-tabler icon-tabler-login-2 IconoLinkSVG" width="35" height="35"
                            viewBox="0 0 24 24" stroke-width="1.5" stroke="#1f9102" fill="none" stroke-linecap="round"
                            stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M9 8v-2a2 2 0 0 1 2 -2h7a2 2 0 0 1 2 2v12a2 2 0 0 1 -2 2h-7a2 2 0 0 1 -2 -2v-2" />
                            <path d="M3 12h13l-3 -3" />
                            <path d="M13 15l3 -3" />
                        </svg>
                        <span class="TextoLinkCalificarAlquiler">Iniciar Sesión</span>
                    </a>
                </div>
                <div class="DivLinkCalificarAlquiler">
                    <a asp-controller="Usuario" asp-action="InformacionUsuario" class="LinkCalificarAlquiler">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-user IconoLinkSVG"
                            width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#020291" fill="none"
                            stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M8 7a4 4 0 1 0 8 0a4 4 0 0 0 -8 0" />
                            <path d="M6 21v-2a4 4 0 0 1 4 -4h4a4 4 0 0 1 4 4v2" />
                        </svg>
                        <span class="TextoLinkCalificarAlquiler">Ver Mi Perfil</span>
                    </a>
                </div>
                <div class="DivLinkCalificarAlquiler">
                    <a asp-controller="Vehiculo" asp-action="InformacionVehiculos" class="LinkCalificarAlquiler">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-car IconoLinkSVG"
                            width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#beac0d" fill="none"
                            stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M7 17m-2 0a2 2 0 1 0 4 0a2 2 0 1 0 -4 0" />
                            <path d="M17 17m-2 0a2 2 0 1 0 4 0a2 2 0 1 0 -4 0" />
                            <path d="M5 17h-2v-6l2 -5h9l4 5h1a2 2 0 0 1 2 2v4h-2m-4 0h-6m-6 -6h15m-6 0v-5" />
                        </svg>
                        <span class="TextoLinkCalificarAlquiler">Ver Mis Vehiculos</span>
                    </a>
                </div>
                <div class="DivLinkCalificarAlquiler">
                    <a asp-controller="Alquiler" asp-action="HistorialAlquileres" class="LinkCalificarAlquiler">
                        <svg xmlns="http://www.w3.org/2000/svg"
                            class="icon icon-tabler icon-tabler-history IconoLinkSVG" width="35" height="35"
                            viewBox="0 0 24 24" stroke-width="1.5" stroke="#000000" fill="none" stroke-linecap="round"
                            stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M12 8l0 4l2 2" />
                            <path d="M3.05 11a9 9 0 1 1 .5 4m-.5 5v-5h5" />
                        </svg>
                        <span class="TextoLinkCalificarAlquiler">Ver Mis Alquileres</span>
                    </a>
                </div>
                <div class="DivLinkCalificarAlquiler">
                    <a asp-controller="Usuario" asp-action="CerrarSesion" class="LinkCalificarAlquiler">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-login IconoLinkSVG"
                            width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#be0d0d" fill="none"
                            stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M15 8v-2a2 2 0 0 0 -2 -2h-7a2 2 0 0 0 -2 2v12a2 2 0 0 0 2 2h7a2 2 0 0 0 2 -2v-2" />
                            <path d="M21 12h-13l3 -3" />
                            <path d="M11 15l-3 -3" />
                        </svg>
                        <span class="TextoLinkCalificarAlquiler">Cerrar Sesión</span>
                    </a>
                </div>
            </nav>
        </div>
        <div class="DivContenidoCalificarAlquiler">
            <div class="DivEncabezadoCalificarAlquiler">
                <header class="EncabezadoCalificarAlquiler">
                    <h1 class="TituloPrincipalCalificarAlquiler">Alquiler de Vehículos</h1>
                </header>
            </div>
            <div class="DivContenidoPrincipalCalificarAlquiler">
                <div class="DivPrincipalCalificarAlquiler">
                    <main role="main" class="PrincipalCalificarAlquiler">
                        <div class="ContenidoPrincipalCalificarAlquiler">
                            <p>Tipo de Vehiculo</p>
                            <p>Clasificacion</p>
                            <p>Marca</p>
                            <p>Linea</p>
                            <p>Modelo</p>
                            <p>Cilindraje</p>
                            <p>Color</p>
                            <p>Ciudad</p>
                            <p>Fecha Inicio</p>
                            <p>Fecha Fin</p>
                            <form>
                                <label>puntucuacion</label>
                                <input type="text" name="name" value="" />
                                <label>Comentario</label>
                                <textarea></textarea>
                                <input type="submit" name="name" value="Calificar Alquiler" />
                            </form>
                        </div>
                    </main>
                </div>
            </div>
            <div class="DivFooterCalificarAlquiler">
                <footer class="FooterCalificarAlquiler">
                    <span class="TextoFooterCalificarAlquiler">
                        &copy; 2024 - Alquiler de Vehículos por Mario Gómez y Juan Diego Claros
                    </span>
                </footer>
            </div>
        </div>
    </div>
</body>
</html>